<nav class="header__inline-menu">
	<ul class="list-menu list-menu--inline" role="list">
	{%- for link in section.settings.menu.links -%}
	
	    {%- if section.blocks.size > 0 -%}
		
			{% for block in section.blocks %}
				{% case block.type %}
					{% when "menu_with_banner" %}
					    {%- if block.settings.text_menu_banner contains link.title -%}
							1-{{ link.title }}
						{%- endif -%}
					{% when "menu_with_product" %}
						{%- if block.settings.text_product contains link.title -%}
							2-{{ link.title }}
						{%- endif -%}
			        {% else %}
						3-{{ link.title | escape }}	
						{% continue %}	
				{% endcase %}
			{% endfor %}

		{%- else -%}
			hellooo	
		{%- endif -%}
	{%- endfor -%}
	</ul>
</nav>